            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/mmsi"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/first_station_MMSI"
                    android:layout_width="wrap_content"
                    android:layout_weight="2"
                    android:layout_height="40sp"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/firstStationRcvLat"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/first_station_received_Latitude"
                    android:layout_width="wrap_content"
                    android:layout_weight="2"
                    android:layout_height="40sp"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/firstStationRcvLong"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/first_station_received_Longitude"
                    android:layout_width="wrap_content"
                    android:layout_weight="2"
                    android:layout_height="40sp"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/firstStationPrdLat"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/first_station_predicted_Latitude"
                    android:layout_width="wrap_content"
                    android:layout_weight="2"
                    android:layout_height="40sp"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/firstStationPrdLong"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/first_station_predicted_Longitude"
                    android:layout_width="wrap_content"
                    android:layout_weight="2"
                    android:layout_height="40sp"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/distanceDiff"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/first_station_diff_distance"
                    android:layout_width="wrap_content"
                    android:layout_weight="2"
                    android:layout_height="40sp"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/updateTime"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/first_station_updateTime"
                    android:layout_width="wrap_content"
                    android:layout_weight="2"
                    android:layout_height="40sp"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/first_station_message_count"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/first_station_msgCount"
                    android:layout_width="wrap_content"
                    android:layout_weight="2"
                    android:layout_height="40sp"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/mmsi"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/second_station_MMSI"
                    android:layout_width="wrap_content"
                    android:layout_weight="2"
                    android:layout_height="40sp"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/secondStationRcvLat"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/second_station_received_latitude"
                    android:layout_width="wrap_content"
                    android:layout_height="40sp"
                    android:layout_weight="2"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/secondStationRcvLong"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/second_station_received_longitude"
                    android:layout_width="wrap_content"
                    android:layout_height="40sp"
                    android:layout_weight="2"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/secondStationPrdLat"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/second_station_predicted_latitude"
                    android:layout_width="0dp"
                    android:layout_height="40sp"
                    android:layout_weight="2"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/secondStationPrdLong"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/second_station_predicted_longitude"
                    android:layout_width="wrap_content"
                    android:layout_height="40sp"
                    android:layout_weight="2"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/distanceDiff"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/second_station_diff_distance"
                    android:layout_width="wrap_content"
                    android:layout_height="40sp"
                    android:layout_weight="2"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/updateTime"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/second_station_updateTime"
                    android:layout_width="wrap_content"
                    android:layout_weight="2"
                    android:layout_height="40sp"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/second_station_message_count"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/second_station_msgCount"
                    android:layout_width="wrap_content"
                    android:layout_weight="2"
                    android:layout_height="40sp"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/predictedBeta"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/calculatedBeta"
                    android:layout_width="wrap_content"
                    android:layout_height="40sp"
                    android:layout_weight="2"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/calculatedBeta"
                    android:textAllCaps="true"
                    android:textSize="15sp"
                    android:layout_marginEnd="15dp"
                    android:textColor="@color/white"/>

                <EditText
                    android:id="@+id/receivedBeta"
                    android:layout_width="wrap_content"
                    android:layout_height="40sp"
                    android:layout_weight="2"
                    android:textSize="15sp"
                    android:textColor="@color/white"
                    android:inputType="none"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/et_bg"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="3">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/betaDifference"
                        android:textAllCaps="true"
                        android:textSize="15sp"
                        android:layout_marginEnd="15dp"
                        android:textColor="@color/white"/>

                    <EditText
                        android:id="@+id/betaDifference"
                        android:layout_width="wrap_content"
                        android:layout_height="40sp"
                        android:layout_weight="2"
                        android:textSize="15sp"
                        android:textColor="@color/white"
                        android:inputType="none"
                        android:paddingLeft="15dp"
                        android:paddingRight="15dp"
                        android:layout_marginTop="5dp"
                        android:background="@drawable/et_bg"/>


                </LinearLayout>


            </LinearLayout>
            <Button
                android:id="@+id/setup_finish"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="@string/next"
                android:enabled="true"
                android:visibility="gone"
                android:onClick="onClickNext"
                android:background="@drawable/btn_bg"
                android:textColor="@color/white"
                android:layout_gravity="right"
                android:textSize="15sp"
                android:clickable="true"
                android:focusable="true"
                android:foreground="?android:attr/selectableItemBackground"/>

        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="10dp">

        <ProgressBar
            android:id="@+id/progressBar"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

        <TextView
            android:id="@+id/progressBarText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="30sp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="68dp"
            android:background="#00000000"
            android:textColor="@color/white" />

    </RelativeLayout>


